CONFIG_PLATFORM_NATIVE=y
# CONFIG_PLATFORM_STM32 is not set
# CONFIG_PLATFORM_GD32 is not set
# CONFIG_PLATFORM_ESP32 is not set
# CONFIG_PLATFORM_NRF52 is not set
CONFIG_PLATFORM_NAME="native"
CONFIG_NATIVE_PLATFORM_NAME="Native Platform (PC Simulation)"
CONFIG_NATIVE_PLATFORM_VERSION="1.0.0"

#
# Native Platform Settings
#
CONFIG_NATIVE_ENABLE_LOGGING=y
CONFIG_NATIVE_LOG_LEVEL=3
CONFIG_NATIVE_ENABLE_STATISTICS=y
CONFIG_NATIVE_BUFFER_ALIGNMENT=4
# end of Native Platform Settings

#
# Resource Managers
#
CONFIG_NATIVE_DMA_CHANNELS=8
CONFIG_NATIVE_ISR_SLOTS=64
# end of Resource Managers

#
# Peripheral Configuration
#
CONFIG_NATIVE_UART_ENABLE=y
CONFIG_NATIVE_UART_MAX_INSTANCES=4
CONFIG_INSTANCE_NATIVE_UART_0=y
CONFIG_UART0_BAUDRATE=115200
CONFIG_UART0_DATA_BITS=8
CONFIG_UART0_STOP_BITS=1
CONFIG_NATIVE_UART0_PARITY_NONE=y
# CONFIG_NATIVE_UART0_PARITY_ODD is not set
# CONFIG_NATIVE_UART0_PARITY_EVEN is not set
CONFIG_UART0_PARITY_VALUE=0
# CONFIG_NATIVE_UART0_MODE_POLLING is not set
CONFIG_NATIVE_UART0_MODE_INTERRUPT=y
# CONFIG_NATIVE_UART0_MODE_DMA is not set
CONFIG_UART0_MODE_VALUE=1
CONFIG_UART0_TX_BUFFER_SIZE=256
CONFIG_UART0_RX_BUFFER_SIZE=256
# CONFIG_INSTANCE_NATIVE_UART_1 is not set
# CONFIG_INSTANCE_NATIVE_UART_2 is not set
# CONFIG_INSTANCE_NATIVE_UART_3 is not set
CONFIG_GPIO_NATIVE=y
CONFIG_GPIO_MAX_PORTS=8
CONFIG_GPIO_MAX_PINS_PER_PORT=16
CONFIG_INSTANCE_NX_GPIOA=y
CONFIG_INSTANCE_NX_GPIOA_PIN0=y
# CONFIG_GPIOA_PIN0_WR_MODE_READ is not set
# CONFIG_GPIOA_PIN0_WR_MODE_WRITE is not set
CONFIG_GPIOA_PIN0_WR_MODE_RW=y
CONFIG_GPIOA_PIN0_MODE=1
CONFIG_GPIOA_PIN0_PULL_VALUE=0
CONFIG_GPIOA_PIN0_SPEED_VALUE=1
CONFIG_GPIOA_PIN0_OUTPUT_VALUE=0
# CONFIG_INSTANCE_NX_GPIOB is not set
# CONFIG_INSTANCE_NX_GPIOC is not set
CONFIG_SPI_NATIVE=y
CONFIG_SPI_MAX_INSTANCES=4
CONFIG_INSTANCE_NX_SPI_0=y
CONFIG_SPI0_MAX_SPEED=1000000
CONFIG_SPI0_MOSI_PIN=11
CONFIG_SPI0_MISO_PIN=12
CONFIG_SPI0_SCK_PIN=13
CONFIG_SPI0_TX_BUFFER_SIZE=256
CONFIG_SPI0_RX_BUFFER_SIZE=256
# CONFIG_INSTANCE_NX_SPI_1 is not set
# CONFIG_INSTANCE_NX_SPI_2 is not set
# CONFIG_INSTANCE_NX_SPI_3 is not set
CONFIG_I2C_NATIVE=y
CONFIG_I2C_MAX_INSTANCES=4
CONFIG_INSTANCE_NX_I2C_0=y
CONFIG_I2C0_SPEED=100000
CONFIG_I2C0_SCL_PIN=5
CONFIG_I2C0_SDA_PIN=4
CONFIG_I2C0_TX_BUFFER_SIZE=256
CONFIG_I2C0_RX_BUFFER_SIZE=256
# CONFIG_INSTANCE_NX_I2C_1 is not set
# CONFIG_INSTANCE_NX_I2C_2 is not set
# CONFIG_INSTANCE_NX_I2C_3 is not set
CONFIG_TIMER_NATIVE=y
CONFIG_TIMER_MAX_INSTANCES=8
CONFIG_INSTANCE_NX_TIMER_0=y
CONFIG_TIMER0_FREQUENCY=1000000
CONFIG_TIMER0_CHANNEL_COUNT=4
# CONFIG_INSTANCE_NX_TIMER_1 is not set
# CONFIG_INSTANCE_NX_TIMER_2 is not set
# CONFIG_INSTANCE_NX_TIMER_3 is not set
# CONFIG_INSTANCE_NX_TIMER_4 is not set
# CONFIG_INSTANCE_NX_TIMER_5 is not set
# CONFIG_INSTANCE_NX_TIMER_6 is not set
# CONFIG_INSTANCE_NX_TIMER_7 is not set
CONFIG_ADC_NATIVE=y
CONFIG_ADC_MAX_INSTANCES=4
CONFIG_INSTANCE_NX_ADC_0=y
CONFIG_ADC0_CHANNEL_COUNT=16
CONFIG_ADC0_RESOLUTION=12
# CONFIG_INSTANCE_NX_ADC_1 is not set
# CONFIG_INSTANCE_NX_ADC_2 is not set
# CONFIG_INSTANCE_NX_ADC_3 is not set
CONFIG_ADC_BUFFER_NATIVE=y
CONFIG_ADC_BUFFER_MAX_INSTANCES=4
CONFIG_INSTANCE_NX_ADC_BUFFER_0=y
CONFIG_ADC_BUFFER0_CHANNEL_COUNT=4
CONFIG_ADC_BUFFER0_BUFFER_SIZE=256
# CONFIG_INSTANCE_NX_ADC_BUFFER_1 is not set
# CONFIG_INSTANCE_NX_ADC_BUFFER_2 is not set
# CONFIG_INSTANCE_NX_ADC_BUFFER_3 is not set
CONFIG_DAC_NATIVE=y
CONFIG_DAC_MAX_INSTANCES=4
CONFIG_INSTANCE_NX_DAC_0=y
CONFIG_DAC0_CHANNEL_COUNT=2
CONFIG_DAC0_RESOLUTION=12
CONFIG_DAC0_VREF_MV=3300
# CONFIG_INSTANCE_NX_DAC_1 is not set
# CONFIG_INSTANCE_NX_DAC_2 is not set
# CONFIG_INSTANCE_NX_DAC_3 is not set
# end of Peripheral Configuration

#
# Hardware Abstraction Layer
#
CONFIG_HAL_ENABLE=y

#
# Debug Configuration
#
# CONFIG_HAL_DEBUG_ENABLE is not set
CONFIG_HAL_ASSERT_ENABLE=y
# end of Debug Configuration

#
# Statistics Configuration
#
CONFIG_HAL_STATISTICS_ENABLE=y
CONFIG_HAL_STATISTICS_BUFFER_SIZE=256
# end of Statistics Configuration

#
# Timeout Configuration
#
CONFIG_HAL_DEFAULT_TIMEOUT_MS=1000
CONFIG_HAL_INIT_TIMEOUT_MS=5000
# end of Timeout Configuration

#
# Memory Configuration
#
CONFIG_HAL_MEM_MODE_STATIC=y
# CONFIG_HAL_MEM_MODE_DYNAMIC is not set
CONFIG_HAL_MEM_POOL_SIZE=4096
# end of Memory Configuration

#
# Thread Safety
#
# end of Thread Safety
# end of Hardware Abstraction Layer

#
# Operating System Abstraction Layer
#
CONFIG_OSAL_BAREMETAL=y
# CONFIG_OSAL_FREERTOS is not set
# CONFIG_OSAL_RTTHREAD is not set
# CONFIG_OSAL_ZEPHYR is not set
# CONFIG_OSAL_LINUX is not set
# CONFIG_OSAL_NATIVE is not set
CONFIG_OSAL_BACKEND_NAME="baremetal"

#
# System Parameters
#
CONFIG_OSAL_TICK_RATE_HZ=1000
CONFIG_OSAL_HEAP_SIZE=16384
CONFIG_OSAL_MAIN_STACK_SIZE=4096
# end of System Parameters

#
# Bare-metal Configuration
#
CONFIG_BAREMETAL_SIMPLE_SCHEDULER=y
CONFIG_BAREMETAL_MAX_TASKS=8
# end of Bare-metal Configuration
# end of Operating System Abstraction Layer
