"                                -DCMAKE_BUILD_TYPE=${params.BUILD_TYPE} \\\n"
"                                -DNEXUS_BUILD_TESTS=ON\n"
"                            cmake --build ${NEXUS_BUILD_DIR}\n"
"                        \"\"\"\n"
"                    }\n"
