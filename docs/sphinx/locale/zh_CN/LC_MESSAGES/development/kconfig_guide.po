#: ../../development/kconfig_guide.rst:2 eaf7b6c293ed4dea818af41610d09e61
msgid "Kconfig Writing Guide"
msgstr ""

#: ../../development/kconfig_guide.rst:4 6632014a31814bd09f84dc30f9a7f3f4
msgid ""
"This guide explains how to write Kconfig files for the Nexus platform, "
"following best practices and conventions."
msgstr ""

#: ../../development/kconfig_guide.rst:8 698d81ad442c4c1f8832586d33e4ad69
msgid "Overview"
msgstr "概述"

#: ../../development/kconfig_guide.rst:10 1940f2c4ecca43a7b271a70f305973f1
msgid ""
"Kconfig is a configuration language used by the Linux kernel and many "
"embedded projects. It provides a hierarchical, type-safe way to define "
"build-time configuration options."
msgstr ""

#: ../../development/kconfig_guide.rst:14 4b7a5700e3e74607bfdbc33c0ddc603d
msgid "**Key Concepts:**"
msgstr "**Key Concepts:**"

#: ../../development/kconfig_guide.rst:16 84f8d61a4942411c96665b6086ec1ae4
msgid "**config**: Define a configuration symbol"
msgstr ""

#: ../../development/kconfig_guide.rst:17 bcf6c26a40184be8b5a468cedc478c33
msgid "**menuconfig**: Define a configuration symbol that opens a menu"
msgstr ""

#: ../../development/kconfig_guide.rst:18 0861a126d30e4e6b9567dfca03daef7a
msgid "**choice**: Define mutually exclusive options"
msgstr ""

#: ../../development/kconfig_guide.rst:19 52182274d61e47df951f300e81f94e1f
msgid "**menu**: Group related options"
msgstr ""

#: ../../development/kconfig_guide.rst:20 4a777f354b5b4f2bb2d9589aefa917a4
msgid "**if/endif**: Conditional inclusion"
msgstr ""

#: ../../development/kconfig_guide.rst:21 7a3e1f78426a46e78cc8e26676cad824
msgid "**depends on**: Specify dependencies"
msgstr ""

#: ../../development/kconfig_guide.rst:22 f524ed76041343548b4f53549604f55c
msgid "**select**: Automatically enable dependencies"
msgstr ""

#: ../../development/kconfig_guide.rst:23 67f7f77cdd6748dc9569e408b3986924
msgid "**default**: Specify default values"
msgstr ""

#: ../../development/kconfig_guide.rst:24 90d7909a753e497593a4ec4776b9af37
msgid "**range**: Specify valid value ranges"
msgstr ""

#: ../../development/kconfig_guide.rst:25 04161f8b515341e2a3130a664b9c8173
msgid "**help**: Provide documentation"
msgstr ""

#: ../../development/kconfig_guide.rst:28 6fd34e6130614b5a92740f3fdb84cbc2
msgid "Basic Syntax"
msgstr ""

#: ../../development/kconfig_guide.rst:31 aec92c82f549461991d773b27e403e7c
msgid "Configuration Symbol"
msgstr "配置 Symbol"

#: ../../development/kconfig_guide.rst:33 77a24bc6eda1457796ebbe1fbed454e5
msgid ""
"config SYMBOL_NAME\n"
"    bool \"Human-readable description\"\n"
"    default y\n"
"    help\n"
"      Detailed help text explaining what this option does.\n"
"      Can span multiple lines."
msgstr ""

#: ../../development/kconfig_guide.rst:42 9944114309234076a4da183930c9a979
msgid "**Types:**"
msgstr "**Types:**"

#: ../../development/kconfig_guide.rst:44 08dda95bd9494829bbed39408d0d15b8
msgid "``bool``: Boolean (y/n)"
msgstr "``bool``: 布尔 (y/n)"

#: ../../development/kconfig_guide.rst:45 74769b894f6e46c6945a2fd10d350692
msgid "``int``: Integer"
msgstr ""

#: ../../development/kconfig_guide.rst:46 742184e18c9b49dbae51af4a88ddceb7
msgid "``hex``: Hexadecimal"
msgstr ""

#: ../../development/kconfig_guide.rst:47 31c3b9eb4f6f4685a4c23cc2393bd879
msgid "``string``: String"
msgstr "``字符串``: String"

#: f359d25990af4294b461049a9f350e7f
msgid "**Example:**"
msgstr "**示例:**"

#: ../../development/kconfig_guide.rst:51 86d06e944eba474da0883329be4c23ee
msgid ""
"config UART1_BAUDRATE\n"
"    int \"UART1 baud rate\"\n"
"    default 115200\n"
"    range 9600 921600\n"
"    help\n"
"      Baud rate for UART1 in bits per second.\n"
"      Common values: 9600, 19200, 38400, 57600, 115200"
msgstr ""

#: ../../development/kconfig_guide.rst:62 5607280ee16a4a1e97cbdcf7f917f53f
msgid "Menu Configuration"
msgstr "Menu 配置"

#: ../../development/kconfig_guide.rst:64 84766e964b1d4731a0471f25f1584917
msgid ""
"``menuconfig`` creates a configuration symbol that also opens a submenu:"
msgstr ""

#: ../../development/kconfig_guide.rst:66 9c3378ac4eb24806b798490a126622a7
msgid ""
"menuconfig UART_ENABLE\n"
"    bool \"Enable UART support\"\n"
"    default y\n"
"    help\n"
"      Enable UART peripheral support.\n"
"\n"
"if UART_ENABLE\n"
"\n"
"config UART_MAX_INSTANCES\n"
"    int \"Maximum UART instances\"\n"
"    default 4\n"
"    range 1 8\n"
"\n"
"endif # UART_ENABLE"
msgstr ""

#: ../../development/kconfig_guide.rst:84 222d2fa9209a4f95aed75b740e43b448
msgid "Choice Menu"
msgstr "选择 菜单"

#: ../../development/kconfig_guide.rst:86 7b9cc5a0656149d580656bbc2c0cac8d
msgid "``choice`` creates mutually exclusive options:"
msgstr ""

#: ../../development/kconfig_guide.rst:88 2fddcc05a614494b8c97a29ea612a7de
msgid ""
"choice\n"
"    prompt \"Target Platform\"\n"
"    default PLATFORM_NATIVE\n"
"    help\n"
"      Select the target hardware platform.\n"
"\n"
"config PLATFORM_NATIVE\n"
"    bool \"Native Platform (PC Simulation)\"\n"
"\n"
"config PLATFORM_STM32\n"
"    bool \"STM32 Platform\"\n"
"\n"
"config PLATFORM_GD32\n"
"    bool \"GD32 Platform\"\n"
"\n"
"endchoice"
msgstr ""
"choice\n"
"    prompt \"Target Platform\"\n"
"    default PLATFORM_NATIVE\n"
"    help\n"
"      Select the target hardware platform.\n"
"\n"
"config PLATFORM_NATIVE\n"
"    bool \"Native Platform (PC Simulation)\"\n"
"\n"
"config PLATFORM_STM32\n"
"    bool \"STM32 Platform\"\n"
"\n"
"config PLATFORM_GD32\n"
"    bool \"GD32 Platform\"\n"
"\n"
"endchoice"

#: ../../development/kconfig_guide.rst:108 9c3a98028d3b4fff90ef713ec30d17e6
msgid "Regular Menu"
msgstr "Regular 菜单"

#: ../../development/kconfig_guide.rst:110 99c0214de9734111ba6620ff5c630ee2
msgid "``menu`` groups related options without creating a symbol:"
msgstr ""

#: ../../development/kconfig_guide.rst:112 0ce21034dd0b42fea3d257d917fd2022
msgid ""
"menu \"System Parameters\"\n"
"\n"
"config TICK_RATE_HZ\n"
"    int \"System tick rate (Hz)\"\n"
"    default 1000\n"
"    range 100 10000\n"
"\n"
"config HEAP_SIZE\n"
"    int \"Heap size (bytes)\"\n"
"    default 32768\n"
"    range 4096 1048576\n"
"\n"
"endmenu"
msgstr ""
"menu \"System Parameters\"\n"
"\n"
"config TICK_RATE_HZ\n"
"    int \"System tick rate (Hz)\"\n"
"    default 1000\n"
"    range 100 10000\n"
"\n"
"config HEAP_SIZE\n"
"    int \"Heap size (bytes)\"\n"
"    default 32768\n"
"    range 4096 1048576\n"
"\n"
"endmenu"

#: b36f573eb53b48cca27cf4f5c1defa7e
msgid "Dependencies"
msgstr "依赖"

#: ../../development/kconfig_guide.rst:132 cdeb0cc9c3d44e4a8b73a195fcdb8ea4
msgid "depends on"
msgstr "依赖于"

#: ../../development/kconfig_guide.rst:134 422c783a7bf94d178f35235ace4047d8
msgid "``depends on`` makes an option visible only when dependencies are met:"
msgstr ""

#: ../../development/kconfig_guide.rst:136 94686d95d35a4d84815d3c8cd39b72c6
msgid ""
"config UART1_DMA_ENABLE\n"
"    bool \"Enable DMA for UART1\"\n"
"    depends on UART1_ENABLE && DMA_ENABLE\n"
"    default y\n"
"    help\n"
"      Use DMA for UART1 transfers."
msgstr ""

#: ../../development/kconfig_guide.rst:145 cffa96f68c534cfbbc1ad2f2fdeb7b69
msgid "**Operators:**"
msgstr "**Operators:**"

#: ../../development/kconfig_guide.rst:147 6587e1c6aff6428090ecb5da912ada9c
msgid "``&&``: Logical AND"
msgstr ""

#: ../../development/kconfig_guide.rst:148 7ba285ff075c42038dd8bd03d58b9041
msgid "``||``: Logical OR"
msgstr ""

#: ../../development/kconfig_guide.rst:149 774ce55ee72c473f91da3331b510c0e9
msgid "``!``: Logical NOT"
msgstr ""

#: ../../development/kconfig_guide.rst:150 b0604108c5ab4c3ca3c9b1cae0cfd8dd
msgid "``=``: Equality"
msgstr "``=``: Equality"

#: ../../development/kconfig_guide.rst:151 c14b65bb286247878bd5f18de4b8c69b
msgid "``!=``: Inequality"
msgstr "``!=``: Inequality"

#: ../../development/kconfig_guide.rst:155 77ffea92aa674a07800dd652b18a3340
msgid ""
"config ADVANCED_FEATURE\n"
"    bool \"Advanced feature\"\n"
"    depends on PLATFORM_STM32 && (STM32F4 || STM32H7)\n"
"    depends on !MINIMAL_BUILD"
msgstr ""
"config ADVANCED_FEATURE\n"
"    bool \"Advanced feature\"\n"
"    depends on PLATFORM_STM32 && (STM32F4 || STM32H7)\n"
"    depends on !MINIMAL_BUILD"

#: ../../development/kconfig_guide.rst:163 e75108b39eb347c683eb6b0ef4dcbf31
msgid "select"
msgstr "选择"

#: ../../development/kconfig_guide.rst:165 cc79650465124b9e86c3f5e51b9aba4b
msgid "``select`` automatically enables dependencies:"
msgstr ""

#: ../../development/kconfig_guide.rst:167 ee46754646ce4e6eba62bf0f9b228c22
msgid ""
"config USB_DEVICE\n"
"    bool \"USB Device support\"\n"
"    select USB_CORE\n"
"    select DMA_ENABLE\n"
"    help\n"
"      Enable USB device functionality."
msgstr ""

#: ../../development/kconfig_guide.rst:176 eb4df45fca6348cc99b27f8b105d63d0
msgid ""
"**Note:** Use ``select`` sparingly. Prefer ``depends on`` when possible."
msgstr ""

#: ../../development/kconfig_guide.rst:179 a744f53d491441fa9a133c21bb12df75
msgid "Conditional Inclusion"
msgstr ""

#: ../../development/kconfig_guide.rst:181 2d91f0ea022d4a3b8e82dfb22ab7c3e5
msgid "``if/endif`` blocks conditionally include configuration options:"
msgstr ""

#: ../../development/kconfig_guide.rst:183 fb8ef864f8794e5abb2990ade1057b2e
msgid ""
"if PLATFORM_STM32\n"
"\n"
"config STM32_CHIP_NAME\n"
"    string \"STM32 chip name\"\n"
"    default \"STM32F407xx\"\n"
"\n"
"source \"platforms/stm32/Kconfig_chip\"\n"
"\n"
"endif # PLATFORM_STM32"
msgstr ""

#: ../../development/kconfig_guide.rst:196 1c0c03edfee041eba23e9577510498db
msgid "Default Values"
msgstr "默认值"

#: ../../development/kconfig_guide.rst:199 e34d61e118cb43bca606260baaf85e9a
msgid "Simple Defaults"
msgstr ""

#: 9eb63ac98ca744fa8d9d116e5e261f94
msgid ""
"config UART1_BAUDRATE\n"
"    int \"UART1 baud rate\"\n"
"    default 115200"
msgstr ""

#: ../../development/kconfig_guide.rst:208 a064dcdfc3e84ba9a43228e05d0feab1
msgid "Conditional Defaults"
msgstr ""

#: ../../development/kconfig_guide.rst:210 500ae6b2491b495886c561376753485f
msgid "Defaults can depend on other options:"
msgstr ""

#: ../../development/kconfig_guide.rst:212 e6e301b307eb4046b7b38be43d5ee5e0
msgid ""
"config HEAP_SIZE\n"
"    int \"Heap size (bytes)\"\n"
"    default 32768 if OSAL_FREERTOS\n"
"    default 16384 if OSAL_BAREMETAL\n"
"    default 65536"
msgstr ""
"config HEAP_SIZE\n"
"    int \"Heap size (bytes)\"\n"
"    default 32768 if OSAL_FREERTOS\n"
"    default 16384 if OSAL_BAREMETAL\n"
"    default 65536"

#: ../../development/kconfig_guide.rst:220 1283cf04a5654b738553526f90d148b5
msgid "**Evaluation order:** First matching condition is used."
msgstr ""

#: ../../development/kconfig_guide.rst:223 abdd9f78acbb404a8dc0b0200eda4081
msgid "Expression Defaults"
msgstr ""

#: ../../development/kconfig_guide.rst:225 3bdbc210989a493bbfa289e7fb27ba09
msgid ""
"config PLATFORM_NAME\n"
"    string\n"
"    default \"native\" if PLATFORM_NATIVE\n"
"    default \"stm32\" if PLATFORM_STM32\n"
"    default \"gd32\" if PLATFORM_GD32"
msgstr ""

#: ../../development/kconfig_guide.rst:234 28ceafe7bbf34e98913b96a49210618f
msgid "Range Constraints"
msgstr "范围 Constraints"

#: ../../development/kconfig_guide.rst:237 4594a1276e4a45498c70049dc438f4a8
msgid "Integer Ranges"
msgstr ""

#: ../../development/kconfig_guide.rst:239 2692ee05282a4de7a2847b85491890a8
msgid ""
"config BUFFER_SIZE\n"
"    int \"Buffer size\"\n"
"    default 256\n"
"    range 16 4096\n"
"    help\n"
"      Buffer size in bytes. Must be between 16 and 4096."
msgstr ""

#: ../../development/kconfig_guide.rst:249 b337dba086764fe6844c6db472974d6a
msgid "Conditional Ranges"
msgstr ""

#: ../../development/kconfig_guide.rst:251 20b4539bbf414e02954869c0e5bfdaf7
msgid ""
"config STACK_SIZE\n"
"    int \"Stack size (bytes)\"\n"
"    default 2048\n"
"    range 512 8192 if PLATFORM_STM32\n"
"    range 1024 16384 if PLATFORM_NATIVE"
msgstr ""
"config STACK_SIZE\n"
"    int \"Stack size (bytes)\"\n"
"    default 2048\n"
"    range 512 8192 if PLATFORM_STM32\n"
"    range 1024 16384 if PLATFORM_NATIVE"

#: ../../development/kconfig_guide.rst:260 4f7adc50621f4b609f6837d5fc4d3124
