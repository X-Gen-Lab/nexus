#: ../../development/kconfig_guide.rst:397 4e0a3ddb6b1646a692077515e9d5f63d
msgid "Configuration Patterns"
msgstr "配置 Patterns"

#: ../../development/kconfig_guide.rst:400 c175471e8948456ebfc33361ac7d3ded
msgid "Platform Configuration"
msgstr "平台配置"

#: e58be8292ac54316baad3067dd9d39e0
msgid "**Pattern:**"
msgstr "**Pattern:**"

#: ../../development/kconfig_guide.rst:404 1c5d3eae08a94112a0539ad140b1d524
msgid ""
"# Platform selection (choice)\n"
"choice\n"
"    prompt \"Target Platform\"\n"
"    default PLATFORM_NATIVE\n"
"\n"
"config PLATFORM_<NAME>\n"
"    bool \"Platform description\"\n"
"\n"
"endchoice\n"
"\n"
"# Platform-specific configuration\n"
"if PLATFORM_<NAME>\n"
"\n"
"# Platform settings\n"
"menu \"Platform Settings\"\n"
"    # ...\n"
"endmenu\n"
"\n"
"# Peripheral configuration\n"
"source \"platforms/<name>/Kconfig_peripherals\"\n"
"\n"
"endif"
msgstr ""
"# Platform selection (choice)\n"
"choice\n"
"    prompt \"Target Platform\"\n"
"    default PLATFORM_NATIVE\n"
"\n"
"config PLATFORM_<NAME>\n"
"    bool \"Platform description\"\n"
"\n"
"endchoice\n"
"\n"
"# Platform-specific configuration\n"
"if PLATFORM_<NAME>\n"
"\n"
