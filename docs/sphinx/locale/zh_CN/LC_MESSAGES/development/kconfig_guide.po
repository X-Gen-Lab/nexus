msgid ""
msgstr ""
"# platforms/stm32/Kconfig\n\n"
"if PLATFORM_STM32\n\n"
"\n\n"
"config STM32_PLATFORM_NAME\n\n"
"    string\n\n"
"    default \STM32 Platform\\n\n"
"\n\n"
"config STM32_PLATFORM_VERSION\n\n"
"    string\n\n"
"    default \1.0.0\\n\n"
"\n\n"
"# 芯片选择\n\n"
"source \platforms/stm32/Kconfig_chip\\n\n"
"\n\n"
"menu \STM32 Platform Settings\\n\n"
"\n\n"
"config STM32_ENABLE_LOGGING\n\n"
"    bool \Enable platform logging\\n\n"
"    default y\n\n"
"\n\n"
"config STM32_LOG_LEVEL\n\n"
"    int \Platform log level\\n\n"
"    default 3\n\n"
"    range 0 4\n\n"
"    depends on STM32_ENABLE_LOGGING\n\n"
"\n\n"
"endmenu\n\n"
"\n\n"
"# 外设配置uration\n\n"
"source \platforms/stm32/Kconfig_peripherals\\n\n"
"\n\n"
"endif # PLATFORM_STM32\n"

