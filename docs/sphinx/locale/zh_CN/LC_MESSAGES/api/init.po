#: ../../api/init.rst:76 046342f74b6c4904a8548cff68f63b05
msgid ""
"#include \"nx_init.h\"\n"
"\n"
"/* Board-level initialization function */\n"
"static int board_clock_init(void) {\n"
"    /* Configure system clock */\n"
"    return 0;  /* Success */\n"
"}\n"
"\n"
"/* Register the function to run at BOARD level */\n"
"NX_INIT_BOARD_EXPORT(board_clock_init);\n"
"\n"
"/* Driver initialization function */\n"
"static int uart_driver_init(void) {\n"
"    /* Initialize UART driver */\n"
"    return 0;  /* Success */\n"
"}\n"
"\n"
"/* Register the function to run at DRIVER level */\n"
"NX_INIT_DRIVER_EXPORT(uart_driver_init);\n"
"\n"
"/* In main() */\n"
"int main(void) {\n"
"    nx_status_t status;\n"
"\n"
"    /* Run all registered initialization functions */\n"
"    status = nx_init_run();\n"
"    if (status != NX_OK) {\n"
"        /* Handle initialization failure */\n"
"    }\n"
"\n"
"    /* Check if all initializations succeeded */\n"
"    if (!nx_init_is_complete()) {\n"
"        /* Some initializations failed */\n"
"        nx_init_stats_t stats;\n"
"        nx_init_get_stats(&stats);\n"
"        /* Log or handle the failure */\n"
"    }\n"
"\n"
"    /* Application code */\n"
"    while (1) {\n"
"        /* ... */\n"
"    }\n"
"}"
msgstr ""
"#include \"nx_init.h\"\n"
"\n"
"/* Board-level initialization function */\n"
"static int board_clock_init(void) {\n"
"    /* Configure system clock */\n"
"    return 0;  /* Success */\n"
"}\n"
"\n"
"/* Register the function to run at BOARD level */\n"
"NX_INIT_BOARD_EXPORT(board_clock_init);\n"
"\n"
"/* Driver initialization function */\n"
"static int uart_driver_init(void) {\n"
"    /* Initialize UART driver */\n"
"    return 0;  /* Success */\n"
"}\n"
"\n"
"/* Register the function to run at DRIVER level */\n"
"NX_INIT_DRIVER_EXPORT(uart_driver_init);\n"
"\n"
"/* In main() */\n"
"int main(void) {\n"
"    nx_status_t status;\n"
"\n"
"    /* Run all registered initialization functions */\n"
"    status = nx_init_run();\n"
"    if (status != NX_OK) {\n"
"        /* Handle initialization failure */\n"
"    }\n"
"\n"
"    /* Check if all initializations succeeded */\n"
"    if (!nx_init_is_complete()) {\n"
"        /* Some initializations failed */\n"
"        nx_init_stats_t stats;\n"
"        nx_init_get_stats(&stats);\n"
"        /* Log or handle the failure */\n"
"    }\n"
"\n"
"    /* Application code */\n"
"    while (1) {\n"
"        /* ... */\n"
"    }\n"
"}"

#: ../../api/init.rst:123 e631eb996ed546b8af63c56f0bfef20a
msgid "Thread Safety"
msgstr "线程 Safety"

#: ../../api/init.rst:125 56d49c3f44ad4faea7e964b51dc877a7
msgid ""
"The Init Framework is **not thread-safe**. All initialization functions "
"should be called during system startup before any threads are created. The "
"``nx_init_run()`` function should only be called once from the main thread."
msgstr ""
"The Init Framework is **not thread-safe**. All initialization functions "
"should be called during system startup before any threads are created. The "
"``nx_init_run()`` function should only be called once from the main thread."

#: ../../api/init.rst:130 b1b25512bf1b4d60b221c6cae6e7fae1
msgid "Notes"
msgstr "注意事项"

#: ../../api/init.rst:132 403ad521fb08416e8dc82dd928171520
msgid ""
"Initialization functions execute in level order (BOARD → PREV → BSP → DRIVER"
" → COMPONENT → APP)"
msgstr ""

#: ../../api/init.rst:133 0716a868286149c0ba585e3cb0757e7b
msgid "Within the same level, execution order is undefined"
msgstr ""

#: ../../api/init.rst:134 f9ff3d8310cb411698ba661ce9437bd3
msgid ""
"If an initialization function fails (returns non-zero), execution continues "
"with remaining functions"
msgstr ""
"If an initialization function fails (returns non-zero), execution continues "
"with remaining functions"

#: ../../api/init.rst:135 b4245770d94c45caa301329f26ea2190
msgid ""
"Use ``nx_init_get_stats()`` to check for failures after ``nx_init_run()``"
msgstr ""
"Use ``nx_init_get_stats()`` to check for failures after ``nx_init_run()``"

