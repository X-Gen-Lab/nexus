#: ../../api/shell.rst:19 a7018385b2f44ba7bc2bbe507af42342
msgid ""
"#include \"shell/shell.h\"\n"
"\n"
"/* Initialize shell with UART backend */\n"
"shell_config_t config = {\n"
"    .prompt = \"nexus> \",\n"
"    .history_size = 10,\n"
"    .max_line_length = 128\n"
"};\n"
"\n"
"shell_init(&config);\n"
"\n"
"/* Start shell processing loop */\n"
"while (1) {\n"
"    shell_process();\n"
"}"
