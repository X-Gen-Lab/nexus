msgid "Basic Configuration"
msgstr "Basic 配置"

#: ../../api/config.rst:19 8afa02d0fbd740de925b5692dd486443
msgid ""
"#include \"config/config.h\"\n"
"\n"
"/* Initialize config manager */\n"
"config_init();\n"
"\n"
"/* Set configuration values */\n"
"int32_t value = 42;\n"
"config_set_int32(\"system.timeout\", value);\n"
"\n"
"config_set_string(\"system.name\", \"MyDevice\");\n"
"\n"
"bool enabled = true;\n"
"config_set_bool(\"feature.enabled\", enabled);\n"
"\n"
"/* Get configuration values */\n"
"int32_t timeout;\n"
"if (config_get_int32(\"system.timeout\", &timeout) == CONFIG_OK) {\n"
"    /* Use timeout value */\n"
"}"
msgstr ""
"#include \"config/config.h\"\n"
"\n"
"/* Initialize config manager */\n"
"config_init();\n"
"\n"
"/* Set configuration values */\n"
"int32_t value = 42;\n"
"config_set_int32(\"system.timeout\", value);\n"
"\n"
"config_set_string(\"system.name\", \"MyDevice\");\n"
"\n"
"bool enabled = true;\n"
"config_set_bool(\"feature.enabled\", enabled);\n"
"\n"
"/* Get configuration values */\n"
"int32_t timeout;\n"
"if (config_get_int32(\"system.timeout\", &timeout) == CONFIG_OK) {\n"
"    /* Use timeout value */\n"
"}"

#: ../../api/config.rst:42 493723d3687e4bf79327d362af9229e6
msgid "Namespaces"
msgstr "命名空间"

#: ../../api/config.rst:44 4a43d2c6561c498fbebafdd5de0536a5
msgid ""
"#include \"config/config.h\"\n"
"\n"
"/* Create namespace */\n"
"config_namespace_create(\"network\");\n"
"\n"
"/* Set values in namespace */\n"
"config_set_string(\"network.ip\", \"192.168.1.100\");\n"
"config_set_int32(\"network.port\", 8080);\n"
"\n"
"/* Iterate namespace */\n"
"config_namespace_iterate(\"network\", my_callback, user_data);"
msgstr ""
"#include \"config/config.h\"\n"
"\n"
"/* Create namespace */\n"
"config_namespace_create(\"network\");\n"
"\n"
"/* Set values in namespace */\n"
"config_set_string(\"network.ip\", \"192.168.1.100\");\n"
"config_set_int32(\"network.port\", 8080);\n"
"\n"
"/* Iterate namespace */\n"
"config_namespace_iterate(\"network\", my_callback, user_data);"

#: 02f8e7c522804f268e3f69d8a77ea481 1bc8825de7f8497fb733d9b6b737f76b
msgid "Flash Backend"
msgstr "Flash 后端"

#: ../../api/config.rst:61 ec98857d7d5f4fe6af970c3560b9a254
msgid ""
"#include \"config/config.h\"\n"
"#include \"config/config_backend.h\"\n"
"\n"
"/* Initialize with Flash backend */\n"
"config_backend* flash_backend = config_flash_backend_create();\n"
"config_set_backend(flash_backend);\n"
"\n"
"/* Configuration is now persisted to Flash */\n"
"config_set_int32(\"system.boot_count\", boot_count);\n"
"\n"
"/* Commit changes to Flash */\n"
"config_commit();"
msgstr ""
"#include \"config/config.h\"\n"
"#include \"config/config_backend.h\"\n"
"\n"
"/* Initialize with Flash backend */\n"
"config_backend* flash_backend = config_flash_backend_create();\n"
"config_set_backend(flash_backend);\n"
"\n"
"/* Configuration is now persisted to Flash */\n"
"config_set_int32(\"system.boot_count\", boot_count);\n"
"\n"
"/* Commit changes to Flash */\n"
"config_commit();"

#: ../../api/config.rst:77 f24023d9fc1642909c8356e71086fa93
msgid "Import/Export"
msgstr "Import/Export"

#: ../../api/config.rst:79 33c446551b0d43dfbe6bc143233d088e
msgid ""
"#include \"config/config.h\"\n"
"\n"
"/* Export configuration to JSON */\n"
"char buffer[1024];\n"
"size_t size = sizeof(buffer);\n"
"config_export_json(buffer, &size);\n"
"\n"
"/* Import configuration from JSON */\n"
"config_import_json(json_string, strlen(json_string));"
msgstr ""
"#include \"config/config.h\"\n"
"\n"
"/* Export configuration to JSON */\n"
"char buffer[1024];\n"
"size_t size = sizeof(buffer);\n"
"config_export_json(buffer, &size);\n"
"\n"
"/* Import configuration from JSON */\n"
"config_import_json(json_string, strlen(json_string));"

