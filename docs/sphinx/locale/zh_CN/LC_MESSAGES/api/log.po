#: ../../api/log.rst:19 b4680ded69084d2fb9c9148a2e0806e7
#, python-format
msgid ""
"#include \"log/log.h\"\n"
"\n"
"/* Initialize log framework */\n"
"log_init();\n"
"\n"
"/* Log messages at different levels */\n"
"LOG_ERROR(\"System\", \"Critical error occurred: %d\", error_code);\n"
"LOG_WARN(\"System\", \"Warning: low memory\");\n"
"LOG_INFO(\"System\", \"System initialized successfully\");\n"
"LOG_DEBUG(\"System\", \"Debug info: value=%d\", value);"
msgstr ""
"#include \"log/log.h\"\n"
"\n"
"/* Initialize log framework */\n"
"log_init();\n"
"\n"
"/* Log messages at different levels */\n"
"LOG_ERROR(\"System\", \"Critical error occurred: %d\", error_code);\n"
"LOG_WARN(\"System\", \"Warning: low memory\");\n"
"LOG_INFO(\"System\", \"System initialized successfully\");\n"
"LOG_DEBUG(\"System\", \"Debug info: value=%d\", value);"

#: ../../api/log.rst:33 61ba5d751df34574afd0d01b5f03b56b
msgid "Module-Specific Logging"
msgstr ""

#: ../../api/log.rst:35 ccc5486935534746a88cb8b7257884a7
#, python-format
msgid ""
"#include \"log/log.h\"\n"
"\n"
"/* Define module name */\n"
"#define MODULE_NAME \"MyModule\"\n"
"\n"
"void my_function(void) {\n"
"    LOG_INFO(MODULE_NAME, \"Function started\");\n"
"\n"
"    /* Your code here */\n"
"\n"
"    LOG_DEBUG(MODULE_NAME, \"Processing data: count=%d\", count);\n"
"}"
msgstr ""
"#include \"log/log.h\"\n"
"\n"
"/* Define module name */\n"
"#define MODULE_NAME \"MyModule\"\n"
"\n"
"void my_function(void) {\n"
"    LOG_INFO(MODULE_NAME, \"Function started\");\n"
"\n"
"    /* Your code here */\n"
"\n"
"    LOG_DEBUG(MODULE_NAME, \"Processing data: count=%d\", count);\n"
"}"

#: ../../api/log.rst:51 bbd84670256448b49b525544a44aed74
msgid "Async Logging"
msgstr ""

#: ../../api/log.rst:53 39caf49f556a4a0283a7d800c0af862b
msgid ""
"#include \"log/log.h\"\n"
"\n"
"/* Configure async mode */\n"
"log_config_t config = {\n"
"    .async_mode = true,\n"
"    .async_buffer_size = 1024,\n"
"    .backend = LOG_BACKEND_UART\n"
"};\n"
"\n"
"log_init_with_config(&config);\n"
"\n"
"/* Log messages - they're buffered and output asynchronously */\n"
"LOG_INFO(\"App\", \"Async logging enabled\");"
msgstr ""
"#include \"log/log.h\"\n"
"\n"
"/* Configure async mode */\n"
"log_config_t config = {\n"
"    .async_mode = true,\n"
"    .async_buffer_size = 1024,\n"
"    .backend = LOG_BACKEND_UART\n"
"};\n"
"\n"
"log_init_with_config(&config);\n"
"\n"
"/* Log messages - they're buffered and output asynchronously */\n"
"LOG_INFO(\"App\", \"Async logging enabled\");"

#: ../../api/log.rst:70 3690e3c693494e86b6855b0b19f5e457
#, fuzzy
msgid "Custom Backend"
msgstr "UART 后端"

#: ../../api/log.rst:72 445ae4296a9d497d88324524fadcf5ef
msgid ""
"#include \"log/log.h\"\n"
"#include \"log/log_backend.h\"\n"
"\n"
"/* Implement custom backend */\n"
"static void my_backend_write(const char* msg, size_t len) {\n"
"    /* Write to custom output */\n"
"}\n"
"\n"
"static log_backend custom_backend = {\n"
"    .write = my_backend_write,\n"
"    .flush = NULL  /* Optional */\n"
"};\n"
"\n"
"/* Register custom backend */\n"
"log_set_backend(&custom_backend);"
msgstr ""
"#include \"log/log.h\"\n"
"#include \"log/log_backend.h\"\n"
"\n"
"/* Implement custom backend */\n"
"static void my_backend_write(const char* msg, size_t len) {\n"
"    /* Write to custom output */\n"
"}\n"
"\n"
"static log_backend custom_backend = {\n"
"    .write = my_backend_write,\n"
"    .flush = NULL  /* Optional */\n"
"};\n"
"\n"
"/* Register custom backend */\n"
"log_set_backend(&custom_backend);"

