msgid "Task Creation"
msgstr "任务 API"

#: ../../api/osal.rst:19 af8686481c44432abe5f948e77209e33
msgid ""
"#include \"osal/osal.h\"\n"
"\n"
"/* Task function */\n"
"void my_task(void* arg) {\n"
"    while (1) {\n"
"        /* Task work */\n"
"        osal_task_delay(1000);  /* Delay 1000ms */\n"
"    }\n"
"}\n"
"\n"
"/* Create task */\n"
"osal_task_t task;\n"
"osal_task_config_t config = {\n"
"    .name = \"MyTask\",\n"
"    .stack_size = 2048,\n"
"    .priority = 5\n"
"};\n"
"\n"
"osal_task_create(&task, my_task, NULL, &config);"
msgstr ""
"#include \"osal/osal.h\"\n"
"\n"
"/* Task function */\n"
"void my_task(void* arg) {\n"
"    while (1) {\n"
"        /* Task work */\n"
