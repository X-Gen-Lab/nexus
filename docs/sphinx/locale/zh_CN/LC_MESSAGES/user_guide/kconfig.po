"menuconfig NATIVE_GPIO_ENABLE\n\n"
"    bool \Enable GPIO support\\n\n"
"    default y\n\n"
"\n\n"
"if NATIVE_GPIO_ENABLE\n\n"
"\n\n"
"menuconfig INSTANCE_NATIVE_GPIO_A\n\n"
"    bool \Enable GPIO Port A\\n\n"
"    default y\n\n"
"\n\n"
