"    bool \"Enable UART support\"\n"
"    default y"
msgstr ""

#: ../../user_guide/kconfig.rst:360 13776f74590749428f1f51cee0286d69
msgid "**Configure UART Instance:**"
msgstr "**配置 UART 实例:**"

#: ../../user_guide/kconfig.rst:362 815048683f624881be2eda10de5c4e73
msgid ""
"if STM32_UART_ENABLE\n"
"\n"
"menuconfig INSTANCE_STM32_UART_1\n"
"    bool \"Enable UART1\"\n"
"    default y\n"
"\n"
"if INSTANCE_STM32_UART_1\n"
"\n"
"config UART1_BAUDRATE\n"
"    int \"UART1 baud rate\"\n"
"    default 115200\n"
"\n"
"config UART1_DATA_BITS\n"
"    int \"UART1 data bits\"\n"
"    default 8\n"
"    range 7 9\n"
"\n"
"choice\n"
"    prompt \"UART1 transfer mode\"\n"
"    default UART1_MODE_DMA\n"
"\n"
"config UART1_MODE_POLLING\n"
"    bool \"Polling\"\n"
