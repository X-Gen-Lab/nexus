#: ../../user_guide/porting.rst:165 a40deaf6fe664f20b60ee56ea05a8975
msgid ""
"/* Memory layout */\n"
"MEMORY\n"
"{\n"
"    FLASH (rx)  : ORIGIN = 0x08000000, LENGTH = 512K\n"
"    RAM (rwx)   : ORIGIN = 0x20000000, LENGTH = 128K\n"
"}\n"
"\n"
"/* Entry point */\n"
"ENTRY(Reset_Handler)\n"
"\n"
"/* Sections */\n"
"SECTIONS\n"
"{\n"
"    .isr_vector :\n"
"    {\n"
"        . = ALIGN(4);\n"
"        KEEP(*(.isr_vector))\n"
"        . = ALIGN(4);\n"
"    } > FLASH\n"
"\n"
"    .text :\n"
"    {\n"
"        . = ALIGN(4);\n"
"        *(.text)\n"
"        *(.text*)\n"
"        . = ALIGN(4);\n"
"    } > FLASH\n"
"\n"
"    /* ... more sections ... */\n"
"}"
msgstr ""

