msgid "Automated Configuration"
msgstr "Automated 配置"

#: ../../user_guide/kconfig_tutorial.rst:647 e57c25f09fc744a7866f10822858e129
msgid "Use scripts for automated configuration:"
msgstr ""

#: ../../user_guide/kconfig_tutorial.rst:649 0b26fcf83e2d48d98b584dfc82785fee
msgid ""
"#!/usr/bin/env python3\n"
"# configure.py\n"
"\n"
"import subprocess\n"
"\n"
"def configure_platform(platform, chip=None):\n"
"    config_lines = [\n"
"        f\"CONFIG_PLATFORM_{platform.upper()}=y\"\n"
"    ]\n"
"\n"
"    if chip:\n"
"        config_lines.append(f\"CONFIG_{chip.upper()}=y\")\n"
"\n"
"    with open('.config', 'w') as f:\n"
"        f.write('\\n'.join(config_lines))\n"
"\n"
"    # Generate header\n"
"    subprocess.run([\n"
"        'python', 'scripts/kconfig/generate_config.py'\n"
"    ])\n"
"\n"
"# Usage\n"
"configure_platform('stm32', 'stm32f407')"
msgstr ""
"#!/usr/bin/env python3\n"
"# configure.py\n"
"\n"
"import subprocess\n"
"\n"
"def configure_platform(platform, chip=None):\n"
"    config_lines = [\n"
"        f\"CONFIG_PLATFORM_{platform.upper()}=y\"\n"
"    ]\n"
"\n"
"    if chip:\n"
"        config_lines.append(f\"CONFIG_{chip.upper()}=y\")\n"
"\n"
"    with open('.config', 'w') as f:\n"
"        f.write('\\n'.join(config_lines))\n"
"\n"
"    # Generate header\n"
"    subprocess.run([\n"
"        'python', 'scripts/kconfig/generate_config.py'\n"
"    ])\n"
"\n"
"# Usage\n"
"configure_platform('stm32', 'stm32f407')"

