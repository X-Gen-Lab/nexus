#: ../../user_guide/kconfig_tutorial.rst:287 95115d2568b74e6c9b03ac26a394c933
msgid "Menu Organization"
msgstr "菜单 Organization"

#: ../../user_guide/kconfig_tutorial.rst:289 f3b72c13c04f424ea25a01383b4882fe
msgid "Use ``menu`` and ``endmenu`` to group related options:"
msgstr ""

#: ../../user_guide/kconfig_tutorial.rst:291 19a001f146ae44f9b721ee0c826aae65
msgid ""
"menu \"UART Configuration\"\n"
"\n"
"config UART_ENABLE\n"
"    bool \"Enable UART support\"\n"
"    default y\n"
"\n"
"if UART_ENABLE\n"
"\n"
"config UART_MAX_INSTANCES\n"
"    int \"Maximum UART instances\"\n"
"    default 4\n"
"    range 1 8\n"
"\n"
"endif # UART_ENABLE\n"
"\n"
"endmenu"
msgstr ""

#: ../../user_guide/kconfig_tutorial.rst:311 48cac9c9e404456ab09705db7ae9c1ca
msgid "Menuconfig"
msgstr "menuconfig"

#: ../../user_guide/kconfig_tutorial.rst:313 af261dedacdf4b3ebb8df94be07ee39b
msgid "Use ``menuconfig`` for options that have sub-options:"
msgstr ""

#: ../../user_guide/kconfig_tutorial.rst:315 123f297e57be4377af7c43c7ca2f4368
msgid ""
"menuconfig INSTANCE_STM32_UART_1\n"
"    bool \"Enable UART1\"\n"
"    default y\n"
"\n"
"if INSTANCE_STM32_UART_1\n"
"\n"
"config UART1_BAUDRATE\n"
"    int \"UART1 baud rate\"\n"
"    default 115200\n"
"\n"
"config UART1_DATA_BITS\n"
"    int \"UART1 data bits\"\n"
"    default 8\n"
"    range 7 9\n"
"\n"
"endif # INSTANCE_STM32_UART_1"
msgstr ""

#: ../../user_guide/kconfig_tutorial.rst:334 a7badbd64c754863aa87d135c996e2cc
msgid ""
"This creates a menu entry that can be enabled/disabled and contains sub-"
"options."
msgstr ""

#: ../../user_guide/kconfig_tutorial.rst:337 f274bc30555e4920b50391df3e118519
msgid "Choice Groups"
msgstr "选择 Groups"

#: ../../user_guide/kconfig_tutorial.rst:339 7aea4daf718d40809e38e09d7e7c6397
msgid "Use ``choice`` for mutually exclusive options:"
msgstr ""

#: ../../user_guide/kconfig_tutorial.rst:341 c72117d1cc534f76bed98bd81894de74
msgid ""
"choice\n"
"    prompt \"UART1 transfer mode\"\n"
"    default UART1_MODE_DMA\n"
"\n"
"config UART1_MODE_POLLING\n"
"    bool \"Polling\"\n"
"    help\n"
"      Use polling mode for UART1 transfers.\n"
"      Simple but blocks CPU.\n"
"\n"
"config UART1_MODE_INTERRUPT\n"
"    bool \"Interrupt\"\n"
"    help\n"
"      Use interrupt mode for UART1 transfers.\n"
"      Non-blocking but requires ISR handling.\n"
"\n"
"config UART1_MODE_DMA\n"
"    bool \"DMA\"\n"
"    help\n"
"      Use DMA mode for UART1 transfers.\n"
"      Most efficient for large transfers.\n"
"\n"
"endchoice"
msgstr ""

#: ../../user_guide/kconfig_tutorial.rst:367 739cc4cbe85f46edbce5c42d105df944
msgid "Only one option in the choice group can be selected."
msgstr ""

#: ../../user_guide/kconfig_tutorial.rst:370 1e2cf923bd584b9db24dba9524577015
msgid "Range Constraints"
msgstr "范围 Constraints"

#: ../../user_guide/kconfig_tutorial.rst:372 ac2a8453ff2746a3b614beb581e628a0
msgid "Use ``range`` to constrain integer values:"
msgstr ""

#: ../../user_guide/kconfig_tutorial.rst:374 22de94c5ef434010829d7fb0e691a796
msgid ""
"config UART1_BAUDRATE\n"
"    int \"UART1 baud rate\"\n"
"    default 115200\n"
"    range 9600 921600\n"
"    help\n"
"      Baud rate must be between 9600 and 921600 bps."
msgstr ""

#: ../../user_guide/kconfig_tutorial.rst:383 a64397efc73342d1a47bc167a7d3abc6
msgid "**Conditional Ranges:**"
msgstr "**Conditional Ranges:**"

#: ../../user_guide/kconfig_tutorial.rst:385 7bb9c26c535f47dabfd007fdbb6bb4a6
msgid ""
"config TIMER_PRESCALER\n"
"    int \"Timer prescaler\"\n"
