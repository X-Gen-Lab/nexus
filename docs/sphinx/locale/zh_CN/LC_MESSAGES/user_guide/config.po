#: ../../user_guide/config.rst:241 f263edb0bd9040198f7f1abe8b2e3689
msgid "Change Notifications"
msgstr "变更通知"

#: ../../user_guide/config.rst:243 009419e8725a4a34acbb64c41c20334f
msgid "**Watch specific key:**"
msgstr "**监听特定键：**"

#: ../../user_guide/config.rst:245 115bbae942664c80a678f5a2eeb50a55
#, python-format
msgid ""
"void on_timeout_change(const char* key, config_type_t type,\n"
"                       const void* old_val, const void* new_val,\n"
"                       void* user_data)\n"
"{\n"
"    int32_t old_timeout = old_val ? *(int32_t*)old_val : 0;\n"
"    int32_t new_timeout = *(int32_t*)new_val;\n"
"    printf(\"Timeout changed: %d -> %d\\n\", old_timeout, new_timeout);\n"
"}\n"
"\n"
"config_cb_handle_t handle;\n"
"config_register_callback(\"app.timeout\", on_timeout_change, NULL, "
"&handle);\n"
"\n"
"// Triggers callback\n"
"config_set_i32(\"app.timeout\", 10000);\n"
"\n"
"// Unregister\n"
"config_unregister_callback(handle);"
msgstr ""

#: ../../user_guide/config.rst:265 26005a43db71463d8af986c07467f951
msgid "**Watch all changes:**"
msgstr "**监听所有变更：**"

#: ../../user_guide/config.rst:267 7b8359e195854f409c257bed05150fde
#, python-format
msgid ""
"void on_any_change(const char* key, config_type_t type,\n"
"                   const void* old_val, const void* new_val,\n"
"                   void* user_data)\n"
"{\n"
"    printf(\"Config changed: %s\\n\", key);\n"
"}\n"
"\n"
"config_cb_handle_t handle;\n"
"config_register_wildcard_callback(on_any_change, NULL, &handle);"
msgstr ""

