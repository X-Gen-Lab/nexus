msgstr "**密钥轮换：**"

#: ../../user_guide/config.rst:418 2628bca6fcee47b9a06be00a0f901270
msgid ""
"// Re-encrypt all encrypted data with new key\n"
"uint8_t new_key[16] = {...};\n"
"config_rotate_encryption_key(new_key, sizeof(new_key), "
"CONFIG_CRYPTO_AES128);"
msgstr ""

#: ../../user_guide/config.rst:425 30b6bcbceb7e4337a88fc8d8835ce41b
msgid "Query Operations"
msgstr "查询操作"

#: ../../user_guide/config.rst:427 0547c14444a44bceb9f485e259bc9061
#, python-format
msgid ""
"// Check if key exists\n"
"bool exists;\n"
"config_exists(\"app.timeout\", &exists);\n"
"\n"
"// Get value type\n"
"config_type_t type;\n"
"config_get_type(\"app.timeout\", &type);\n"
"\n"
"// Delete key\n"
"config_delete(\"app.timeout\");\n"
"\n"
"// Get key count\n"
"size_t count;\n"
"config_get_count(&count);\n"
"\n"
"// Iterate all entries\n"
"bool print_entry(const config_entry_info_t* info, void* user_data)\n"
"{\n"
"    printf(\"Key: %s, Type: %d, Size: %d\\n\",\n"
"           info->key, info->type, info->value_size);\n"
"    return true;  // Continue iteration\n"
"}\n"
"\n"
"config_iterate(print_entry, NULL);"
msgstr ""

