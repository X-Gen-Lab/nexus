msgstr ""

#: ../../user_guide/log.rst:278 f52f7f4faa5d46888db65218b5e0ea4c
msgid "Backend-Level Filtering"
msgstr "后端级别过滤"

#: ../../user_guide/log.rst:280 b65ec79e88474eb79f51f7dbb7fe6c8b
msgid "Each backend can have its own minimum level:"
msgstr ""

#: ../../user_guide/log.rst:282 5ee479e42f7f45639064f7d5bc429b72
msgid ""
"log_init(NULL);\n"
"log_set_level(LOG_LEVEL_TRACE);  // Global: allow all\n"
"\n"
"// Console shows all messages\n"
"log_backend_t* console = log_backend_console_create();\n"
"console->min_level = LOG_LEVEL_TRACE;\n"
"log_backend_register(console);\n"
"\n"
"// UART only shows WARN and above\n"
"log_backend_t* uart = log_backend_uart_create(HAL_UART_0);\n"
"uart->min_level = LOG_LEVEL_WARN;\n"
"log_backend_register(uart);\n"
"\n"
"LOG_DEBUG(\"Only to console\");\n"
"LOG_WARN(\"To console and UART\");"
msgstr ""

#: ../../user_guide/log.rst:301 a36877d21ba2495381ac596c120a7a05
msgid "Runtime Reconfiguration"
msgstr "运行时重配置"

#: ../../user_guide/log.rst:303 691accabc41c4134a7d6d69fc10e6ef5
msgid "Log settings can be changed at runtime:"
msgstr ""

#: ../../user_guide/log.rst:305 41e6ed3834d745aa81766e72aa9b6087
msgid ""
"// Change log level\n"
"log_set_level(LOG_LEVEL_DEBUG);\n"
"\n"
"// Change format\n"
"log_set_format(\"[%l] %m\");\n"
"\n"
"// Change max message length\n"
"log_set_max_msg_len(64);\n"
