#: ../../getting_started/quickstart.rst:90 f0b2baabd4d34634bd02b5a77bd47445
msgid "Build for STM32F4"
msgstr "构建 STM32F4 版本"

#: ../../getting_started/quickstart.rst:92 a586d00b58d740b0ac9ae81af6b7a9f0
msgid ""
"# Configure for STM32F4\n"
"cmake -B build-stm32f4 \\\n"
"    -DCMAKE_TOOLCHAIN_FILE=nexus/cmake/toolchains/arm-none-eabi.cmake \\\n"
"    -DNEXUS_PLATFORM=stm32f4\n"
"\n"
"# Build\n"
"cmake --build build-stm32f4\n"
"\n"
"# Flash (using OpenOCD)\n"
"openocd -f interface/stlink.cfg -f target/stm32f4x.cfg \\\n"
"    -c \"program build-stm32f4/blinky.elf verify reset exit\""
msgstr ""
"# Configure for STM32F4\n"
"cmake -B build-stm32f4 \\\n"
"    -DCMAKE_TOOLCHAIN_FILE=nexus/cmake/toolchains/arm-none-eabi.cmake \\\n"
"    -DNEXUS_PLATFORM=stm32f4\n"
"\n"
"# Build\n"
"cmake --build build-stm32f4\n"
"\n"
"# Flash (using OpenOCD)\n"
"openocd -f interface/stlink.cfg -f target/stm32f4x.cfg \\\n"
"    -c \"program build-stm32f4/blinky.elf verify reset exit\""

