#: ../../getting_started/quickstart.rst:35 a1c31b52a64944af8d31563bb59efb52
msgid "Create ``main.c``:"
msgstr ""

#: ../../getting_started/quickstart.rst:37 b6ce9329eedd42d888e4a1feaa60faf0
msgid ""
"#include \"hal/nx_hal.h\"\n"
"\n"
"#define LED_PORT    0   /* Port A */\n"
"#define LED_PIN     5   /* Pin 5 */\n"
"\n"
"int main(void)\n"
"{\n"
"    /* Initialize HAL */\n"
"    nx_hal_init();\n"
"\n"
"    /* Configure GPIO */\n"
"    nx_gpio_config_t cfg = {\n"
"        .mode  = NX_GPIO_MODE_OUTPUT_PP,\n"
"        .pull  = NX_GPIO_PULL_NONE,\n"
"        .speed = NX_GPIO_SPEED_LOW,\n"
"    };\n"
"\n"
"    /* Get GPIO device */\n"
"    nx_gpio_t* led = nx_factory_gpio_with_config(LED_PORT, LED_PIN, &cfg);\n"
"    if (!led) {\n"
"        return -1;\n"
"    }\n"
"\n"
"    /* Blink loop */\n"
"    while (1) {\n"
"        led->toggle(led);\n"
"        /* Platform-specific delay */\n"
"        for (volatile int i = 0; i < 1000000; i++);\n"
"    }\n"
"\n"
"    /* Cleanup (never reached) */\n"
"    nx_factory_gpio_release(led);\n"
"    nx_hal_deinit();\n"
"    return 0;\n"
"}"
msgstr ""
"#include \"hal/nx_hal.h\"\n"
"\n"
"#define LED_PORT    0   /* Port A */\n"
"#define LED_PIN     5   /* Pin 5 */\n"
"\n"
