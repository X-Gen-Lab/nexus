"#include \"hal/nx_status.h\"\n"
"\n"
"nx_status_t my_function(void) {\n"
"    nx_status_t result = NX_OK;\n"
"    void* resource = NULL;\n"
"\n"
"    /* Allocate resource */\n"
"    resource = allocate();\n"
"    if (!resource) {\n"
"        result = NX_ERR_NO_MEMORY;\n"
"        goto cleanup;\n"
"    }\n"
"\n"
"    /* Use resource */\n"
"    NX_GOTO_IF_ERROR(operation(resource), cleanup, result);\n"
"\n"
"cleanup:\n"
"    if (resource) {\n"
"        free(resource);\n"
"    }\n"
"    return result;\n"
"}"
msgstr ""
