"    .cmd_buffer_size = 128,\n"
"    .history_depth = 16,\n"
"    .max_commands = 32\n"
"};\n"
"\n"
"/* Initialize shell */\n"
"shell_init(&config);\n"
"\n"
"/* Set UART backend */\n"
"shell_set_backend(&shell_uart_backend);\n"
"\n"
"/* Register commands */\n"
"shell_register_builtin_commands();\n"
"shell_register_command(&cmd_led_def);"

