#: ../../platform_guides/stm32f4.rst:125 5d9f24ea8b9345f5b3824c726959a6e4
msgid "Basic Build"
msgstr "Basic 构建"

#: ../../platform_guides/stm32f4.rst:127 bbc73180e8bc425cb773ed086295ff00
msgid ""
"# Configure for STM32F4 platform\n"
"cmake -B build -DPLATFORM=stm32 -DSTM32_CHIP=STM32F407 \\\n"
"      -DCMAKE_TOOLCHAIN_FILE=cmake/toolchains/arm-none-eabi.cmake\n"
"\n"
"# Build\n"
"cmake --build build\n"
"\n"
"# Generate binary\n"
"arm-none-eabi-objcopy -O binary build/app.elf build/app.bin"
msgstr ""
"# Configure for STM32F4 platform\n"
"cmake -B build -DPLATFORM=stm32 -DSTM32_CHIP=STM32F407 \\\n"
"      -DCMAKE_TOOLCHAIN_FILE=cmake/toolchains/arm-none-eabi.cmake\n"
"\n"
"# Build\n"
"cmake --build build\n"
"\n"
"# Generate binary\n"
"arm-none-eabi-objcopy -O binary build/app.elf build/app.bin"

