"    /* Enable Ethernet */\n"
"    eth->enable(eth);\n"
"\n"
"    /* Transmit packet */\n"
"    uint8_t packet[64];\n"
"    /* Fill packet data */\n"
"    eth->transmit(eth, packet, sizeof(packet));\n"
"}"
