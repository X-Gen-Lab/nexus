#: ../../platform_guides/gd32.rst:378 f279fa9859234406a87343ec788576a9
msgid "FreeRTOS Task Example"
msgstr "FreeRTOS 任务 示例"

#: ../../platform_guides/gd32.rst:380 1d20d8a1c94c4a1db2fd2bfd7858761f
msgid ""
"#include \"osal/nx_osal.h\"\n"
"#include \"hal/nx_gpio.h\"\n"
"\n"
"void led_task(void* param) {\n"
"    nx_gpio_t* gpio = nx_factory_gpio(0);\n"
"    nx_lifecycle_t* lc = gpio->get_lifecycle(gpio);\n"
"    lc->init(lc);\n"
"\n"
"    /* Configure LED pin */\n"
"    nx_gpio_config_t config = {\n"
"        .pin = 7,  /* PD7 on GD32F407V-START */\n"
"        .mode = NX_GPIO_MODE_OUTPUT,\n"
"        .pull = NX_GPIO_PULL_NONE,\n"
"        .speed = NX_GPIO_SPEED_LOW\n"
"    };\n"
"    gpio->configure(gpio, &config);\n"
"\n"
"    /* Blink LED */\n"
"    while (1) {\n"
"        gpio->write_pin(gpio, 7, 1);\n"
"        nx_osal_delay_ms(500);\n"
"        gpio->write_pin(gpio, 7, 0);\n"
"        nx_osal_delay_ms(500);\n"
"    }\n"
"}\n"
"\n"
"int main(void) {\n"
"    nx_osal_init();\n"
"\n"
"    /* Create task */\n"
"    nx_osal_task_create(led_task, \"LED\", 512, NULL, 1, NULL);\n"
"\n"
"    /* Start scheduler */\n"
"    nx_osal_start();\n"
"    return 0;\n"
"}"
