#: ../../platform_guides/gd32.rst:126 903656eb72794aa58a5d4c2689514795
msgid "Basic Build"
msgstr "Basic 构建"

#: ../../platform_guides/gd32.rst:128 cbc64b2d98894c57ad5d0b27936f2983
msgid ""
"# Configure for GD32 platform\n"
"cmake -B build -DPLATFORM=gd32 -DGD32_CHIP=GD32F407 \\\n"
"      -DCMAKE_TOOLCHAIN_FILE=cmake/toolchains/arm-none-eabi.cmake\n"
"\n"
"# Build\n"
"cmake --build build\n"
"\n"
"# Generate binary\n"
"arm-none-eabi-objcopy -O binary build/app.elf build/app.bin"
msgstr ""
"# Configure for GD32 platform\n"
"cmake -B build -DPLATFORM=gd32 -DGD32_CHIP=GD32F407 \\\n"
"      -DCMAKE_TOOLCHAIN_FILE=cmake/toolchains/arm-none-eabi.cmake\n"
"\n"
"# Build\n"
"cmake --build build\n"
"\n"
"# Generate binary\n"
"arm-none-eabi-objcopy -O binary build/app.elf build/app.bin"

